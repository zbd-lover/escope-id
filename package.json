{
  "name": "escope-id",
  "version": "1.1.0",
  "description": "Analyze the definition of identifier based on estree",
  "main": "lib/escope-id.js",
  "unpkg": "dist/escope-id.js",
  "module": "es/escope-id.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "lib",
    "es",
    "types",
    "src/index.ts",
    "src/analyze.ts",
    "src/scope.ts"
  ],
  "scripts": {
    "test": "jest",
    "clean": "rimraf lib dist es types",
    "coverage": "jest --coverage",
    "tsd": "tsc --declaration true --noEmit false --emitDeclarationOnly true --declarationDir ./types",
    "lint": "eslint --ext ts src types",
    "fix:lint": "eslint  --fix ./src ./types",
    "prepublish": "npm run clean && npm run tsd && npm run test && npm run lint && npm run fix:lint",
    "build": "rollup -c"
  },
  "keywords": [
    "escope",
    "escope-id",
    "identifier",
    "analysis of identifier",
    "analysis of class",
    "analysis of scope",
    "estree"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:zbd-lover/escope-id.git"
  },
  "author": "BaoDi Zheng",
  "license": "MIT",
  "dependencies": {
    "estraverse": "^5.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.6",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-typescript": "^7.18.6",
    "@rollup/plugin-babel": "^6.0.2",
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.1",
    "@rollup/plugin-typescript": "^9.0.2",
    "@types/estraverse": "^5.1.2",
    "@types/jest": "^29.2.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@types/estree": "^1.0.0",
    "acorn": "^8.8.1",
    "babel-jest": "^29.2.2",
    "eslint": "^8.26.0",
    "jest": "^29.2.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.1",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  }
}